<header class="page-header">
<h1>Please Login</h1>
</header>
<div>
<%= form_tag '/login' do %>

  <div class="form-group">
    Email: <%= text_field_tag :email %>
  </div>
  <div class="form-group">
    Password: <%= password_field_tag :password %>
  </div>
  <div class="form-group">
    <input type="submit" value="Submit" class="btn btn-primary">
  </div>
<% end %>
</div>
